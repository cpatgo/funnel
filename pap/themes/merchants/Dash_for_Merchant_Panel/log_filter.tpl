<!-- log_filter -->
    
<div class="LogFilter">
 		
  <div class="FilterRow">
      <div class="FilterLegend">##Date created##</div>
     {widget id="created"}
     <div class="clear"></div>
  </div>
  
  <div class="FilterRow">
      <div class="FilterLegend">##Time##</div>
     {widget id="time_created"}{widget id="time"}
     <div class="clear"></div>
  </div>
  
  <div class="FilterRow">
      <div class="FilterLegend">##Log level##</div>
     {widget id="level"}
     <div class="clear"></div>
  </div>
  
  <div class="FilterRow">
      <div class="FilterLegend">##Custom##</div>
     {widget id="custom"}
     <div class="clear"></div>
  </div>

</div>
        
    
