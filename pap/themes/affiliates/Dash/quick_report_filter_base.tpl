<!-- quick_report_filter_base -->
<div class="ReportsFilter">

    <div class="FilterRow">
        <div class="FilterLegend">##Channel##</div>
        {widget id="channel"}
        <div class="clear"></div>
    </div>

	<div class="FilterRow">
		<div class="FilterLegend">##Campaign##</div>
		{widget id="campaignid"}
		<div class="clear"></div>
	</div>

	<div class="FilterRow">
		<div class="FilterLegend">##Destination URL##</div>
		{widget id="destinationurl"}
		<div class="clear"></div>
	</div>
	<div class="FilterRow">
		<div class="FilterLegend">##Date##</div>
    		{widget id="datetime"}
		<div class="clear"></div>
	</div>

</div>

<div class="clear"></div>

