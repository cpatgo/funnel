<!-- transaction_list_filter_base -->

<div class="TransactionsFilter">

    <div class="FilterRow">
        <div class="FilterLegend">##Channel##</div>
        {widget id="channelValue"}
        <div class="clear"></div>
    </div>

    <div class="FilterRow">
        <div class="FilterLegend">##Campaign##</div>
        {widget id="campaignid"}
        <div class="clear"></div>
    </div>

    <div class="FilterRow">
        <div class="FilterLegend">##Banner##</div>
        {widget id="bannerid"}
        <div class="clear"></div>
    </div>

	<div class="FilterRow">
		<div class="FilterLegend">##Date created##</div>
		{widget id="dateinserted"}
		<div class="clear"></div>
	</div>

	<div class="FilterRow">
        <div class="FilterLegend">##Order ID##</div>
        {widget id="orderId"}
        ##You can input multiple order IDs separated either by new line or comma, use '%' for like search##
        <div class="clear"></div>
	</div>

	<div class="FilterRow">
		<div class="FilterLegend">##Status##</div>
		{widget id="rstatus"}
		<div class="clear"></div>
	</div>

	<div class="FilterRow">
		<div class="FilterLegend">##Type##</div>
		{widget id="rtype"}
		<div class="clear"></div>
	</div>

    <div class="FilterRow">
        <div class="FilterLegend">##Payout status##</div>
        {widget id="payoutstatus"}
        <div class="clear"></div>
    </div>

	<div class="FilterRow">
		<div class="FilterLegend">##Custom##</div>
		{widget id="custom"}
		<div class="clear"></div>
	</div>

</div>

<div style="clear: both;"></div>
