<!--    banner_widget_header   -->
<table class="BannerHeader" width="100%" padding="2" style="margin-left: 8px;">
<tr>
  <td style="color: #000000" class="TextAlignLeft" colspan="2">
  	<div class="BannerTypeLabel">{widget id="bannerTypeLabel"}</div>
  </td>
  <td>##Banner name: ##</td>
  <td class="TextAlignLeft" style="color: #000000">{widget id="bannerNameLabel" class="Inline"}</td>
</tr><tr>
  <td>##Campaign: ##</td>
  <td class="TextAlignLeft" style="color: #000000">{widget id="campaignNameLabel" class="Inline"}</td>

  <td class="TextAlignLeft">##Target: ##</td>
  <td style="color: #000000" class="TextAlignLeft">{widget id="bannerTargetUrlLabel" class="Inline"}</td>
</tr>
<tr>
  <td>##Commissions: ##</td>
  <td style="color: #000000" class="TextAlignLeft" colspan="3">{widget id="campaignDetailsLabel" class="Inline"}</td>
</tr>
</table>
